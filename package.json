{
  "name": "stages_fst",
  "version": "1.0.0",
  "description": "Projet de renouvellement du site de stages de la FST",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Daniel Wettel",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.1",
    "mail-listener-next": "^1.0.5",
    "mail-listener4": "^1.1.3",
    "mongoose": "^5.0.15",
    "nodemailer": "^4.6.5",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "nodemon": "^1.17.3"
  }
}
